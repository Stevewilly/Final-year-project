<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#FF4500" />
            <gradient android:angle="-90" android:startColor="#e29413" android:endColor="#FF4500"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#FF4500" />
            <solid android:color="#e29413"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#e29413" />
            <gradient android:angle="-90" android:startColor="#e29413" android:endColor="#58857e" />
        </shape>
    </item>
</selector>